import java.util.Scanner;

public class DeseneGoreMethod {
    public static void main(String[] args) {
        System.out.println("Desen olusturmak istediginiz sayiyi giriniz: ");
        Scanner scan = new Scanner(System.in);
        int desen = scan.nextInt();
        System.out.println("N sayisi :" + desen);

        int[] dizi = new int[1];
        dizi[0] = desen;
       int temp = desen;
        while (temp > 0) {
            System.out.print(temp + " ");
            temp -= 5;
        }

        // 0 veya negatif olan say覺
        System.out.print(temp + " ");

        // Art覺rma k覺sm覺
        while (temp < dizi[0]) {
            temp += 5;
            System.out.print(temp + " ");
        }
        
            
        
    }
}
